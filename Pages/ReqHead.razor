@page "/counter"
@using BlazorApp1.Data
@inject ValuesService VService

@if (results == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <FormsValidation dirs="@results.Data.Dirs" />

    @*@foreach (string s in results.Data.Dirs)
        {
            @s

        }*@
}

@code {

    public Rootobject results { get; set; }
    [Parameter]
    public Dictionary<string, string> reqFields { get; set; }


    protected override async Task OnInitializedAsync()
    {
        results = await VService.GetAsync();

    }

    private string item;

    [Parameter]
    public EventCallback<string> OnClickCallback { get; set; }

    void OnClick(EventArgs e)
    {
        OnClickCallback.InvokeAsync(item);
    }


}


