@page "/fetchdata"

@using BlazorApp1.Data
@inject ValuesService VService

@if (results == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>

            <tr>
                @foreach (string s in results.Data.Dirs)
                {
                    <th>@s</th>
                }
                </tr>
        </thead>

        <tbody>
            @foreach (string s in results.Data.Dirs)
            {
                <tr>
                    @s

                </tr>
            }


        </tbody>
    </table>
}

@code {

    public Rootobject results { get; set; }

}
